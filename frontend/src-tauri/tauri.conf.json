{
  "build": {
    "beforeDevCommand": "trunk serve",
    "beforeBuildCommand": "trunk build",
    "frontendDist": "../dist",
    "devUrl": "http://localhost:3001"
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ]
  },
  "productName": "rust-todo-frontend",
  "mainBinaryName": "rust-todo-frontend",
  "version": "0.0.0",
  "identifier": "com.github.ritz-p",
  "plugins": {},
  "app": {
    "withGlobalTauri": true,
    "windows": [
      {
        "title": "rust-todo-frontend",
        "width": 1080,
        "height": 720,
        "useHttpsScheme": true,
        "decorations": false,
        "resizable": true,
        "closable": true,
        "maximizable": true,
        "minimizable": true
      }
    ],
    "security": {
      "csp": null
    }
  }
}